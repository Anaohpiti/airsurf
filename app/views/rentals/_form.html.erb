<%= simple_form_for [ @board, @rental ] do |f| %>

  <%= f.input :start_date, as: :string, input_html: {type: :date} %>
  <%= f.input :end_date, as: :string, input_html: {type: :date} %>
  <%= f.submit "Book your board!", class: "text-center btn btn-lg btn-danger justify-content-self rounded"  %>
<% end %>
