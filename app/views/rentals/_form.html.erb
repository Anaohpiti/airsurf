<%#= raise %>
<%= simple_form_for [ @board, @rental ], html: {data: {blocked_dates: @blocked_dates.to_json}} do |f| %>

  <%= f.input :start_date, as: :string, input_html: {class: "datepicker"} %>
  <%= f.input :end_date, as: :string, input_html: {class: "datepicker"} %>
  <%= f.submit "Book", class: "text-center btn btn-lg btn-danger justify-content-self rounded"  %>
<% end %>
